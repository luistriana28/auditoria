<!DOCTYPE HTML>
<html>
  <head>
    <title>Supernova Consulting</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <link href="/template/css/bootstrap.min.css" rel='stylesheet' type='text/css'/>
    <!-- Custom Theme files -->
    <link href="/template/css/style.css" rel='stylesheet' type='text/css'/>
    <link href="/template/css/font-awesome.css" rel="stylesheet">
    <link href="/template/css/custom.css" rel="stylesheet">
    {% block css %}

    {% endblock %}

  </head>
  <body>
    <div id="wrapper">
      <nav class="navbar-default navbar-static-top" role="navigation">
        <div class="navbar-header">
          <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
          <h1>
            <a class="navbar-brand" href="javascript:void(0)">Supernova</a>
          </h1>
        </div>
        <div class=" border-bottom">
          <div class="full-left">
            <section class="full-top">
              <button id="toggle">
                <i class="fa fa-arrows-alt"></i>
              </button>
            </section>
            <div class="clearfix"></div>
          </div>

          <div class="drop-men">
            <ul class=" nav_1">
              <li class="dropdown">
                <a href="#" class="dropdown-toggle dropdown-at" data-toggle="dropdown">
                  <span class=" name-caret">
                    {% block username %}

                    {% endblock %}&nbsp;
                    <i class="caret"></i>
                  </span>
                  <img src="/template/images/wo.jpg">
                </a>
                <ul class="dropdown-menu " role="menu">
                  <li>
                    <a href="/logout">
                      <i class="fa fa-caret-right"></i>
                      Salir
                    </a>
                  </li>
                </ul>
              </li>

            </ul>
          </div>
          <div class="clearfix"></div>
          <div class="navbar-default sidebar" role="navigation">
            <div class="sidebar-nav navbar-collapse">
              <ul class="nav" id="side-menu">
                <li>
                  <a href="/dashboard" class=" hvr-bounce-to-right">
                    <i class="fa fa-dashboard nav_icon "></i>
                    <span class="nav-label">Dashboard</span>
                  </a>
                </li>
                <li>
                  <a href="#" class=" hvr-bounce-to-right">
                    <i class="fa fa-indent nav_icon"></i>
                    <span class="nav-label">Gestion</span>
                    <span class="fa arrow"></span>
                  </a>
                  <ul class="nav nav-second-level">
                    <li>
                      <a href="/empresas" class=" hvr-bounce-to-right">
                        <i class="fa fa-area-chart nav_icon"></i>Empresas</a>
                    </li>
                    <li>
                      <a href="/auditor" class=" hvr-bounce-to-right">
                        <i class="fa fa-map-marker nav_icon"></i>Auditorias</a>
                    </li>
                    <li>
                      <a href="/departamentos" class=" hvr-bounce-to-right">
                        <i class="fa fa-map-marker nav_icon"></i>Departamento</a>
                    </li>
                  </ul>
                </li>
              </ul>
            </div>
          </div>
        </nav>
        <div id="page-wrapper" class="gray-bg dashbard-1">
          <div class="content-main">
            <div class="banner">
              <h2>
                {% block title %}

                {% endblock %}
              </h2>
            </div>
            <div class="blank">
              <div class="blank-page">
                {% block content %}

                {% endblock %}
              </div>
            </div>
          </div>
        </div>
        <div class="clearfix"></div>
      </div>

      <script type="text/javascript" src="/libs/jQuery/jquery-3.2.1.min.js"></script>
      <script src="/template/js/bootstrap.min.js"></script>
      <!-- Mainly scripts -->
      <script src="/template/js/jquery.metisMenu.js"></script>
      <script src="/template/js/jquery.slimscroll.min.js"></script>
      <!-- Custom and plugin javascript -->
      <script src="/template/js/custom.js"></script>
      <script src="/template/js/screenfull.js"></script>
      <script>
        $(function () {
          $('#supported').text('Supported/allowed: ' + !!screenfull.enabled);

          if (!screenfull.enabled) {
            return false;
          }

          $('#toggle').click(function () {
            screenfull.toggle($('#container')[0]);
          });

        });
      </script>
      <script src="/template/js/jquery.nicescroll.js"></script>
      <script src="/template/js/scripts.js"></script>
      {% block js %}

      {% endblock %}
    </body>
  </html>
